[global_config]
  dbus = False
  handle_size = 2
  title_hide_sizetext = True
  inactive_color_offset = 0.8282828282828283
  enabled_plugins = LaunchpadBugURLHandler, LaunchpadCodeURLHandler, APTURLHandler, TerminalShot, TerminatorThemes
  always_split_with_profile = True
[keybindings]
  split_horiz = <Primary><Shift>h
  split_vert = <Primary><Shift>j
[profiles]
  [[default]]
    audible_bell = True
    background_type = transparent
    cursor_color = "#ffffff"
    font = Fira Code Medium 13
    foreground_color = "#ffffff"
    show_titlebar = False
    scrollbar_position = hidden
    palette = "#1f2229:#d41919:#5ebdab:#fea44c:#367bf0:#9755b3:#49aee6:#e6e6e6:#198388:#ec0101:#47d4b9:#ff8a18:#277fff:#962ac3:#05a1f7:#ffffff"
    bold_is_bright = True
    copy_on_selection = True
  [[Aci]]
    audible_bell = True
    background_color = "#191b1d"
    background_darkness = 1.0
    background_type = transparent
    cursor_color = "#c4e9ff"
    font = Source Code Pro 13
    foreground_color = "#ffffff"
    show_titlebar = False
    scrollbar_position = hidden
    palette = "#363636:#ff0883:#83ff08:#ff8308:#0883ff:#8308ff:#08ff83:#b6b6b6:#363636:#ff0883:#83ff08:#ff8308:#0883ff:#8308ff:#08ff83:#b6b6b6"
    use_system_font = False
    use_theme_colors = True
    copy_on_selection = True
  [[deep]]
    audible_bell = True
    background_color = "#090909"
    background_darkness = 1.0
    background_type = transparent
    cursor_color = "#d0d0d0"
    font = Source Code Pro 13
    foreground_color = "#cdcdcd"
    show_titlebar = False
    scrollbar_position = hidden
    palette = "#000000:#d70005:#1cd915:#d9bd26:#5665ff:#b052da:#50d2da:#e0e0e0:#535353:#fb0007:#22ff18:#fedc2b:#9fa9ff:#e09aff:#8df9ff:#ffffff"
    use_system_font = False
    use_theme_colors = True
    copy_on_selection = True
    background_image = /home/elliot/Pictures/dragon.png
[layouts]
  [[default]]
    [[[window0]]]
      type = Window
      parent = ""
    [[[child1]]]
      type = Terminal
      parent = window0
      profile = default
[plugins]
